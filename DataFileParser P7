string name;

int main() {
  //Reading and outputting a csv file
      string Myfile;
      string Info;

      ifstream MyReadFile("file.csv");
      //Read file

      std::cout << "\n\nInitial\t\t\t\tLast\t\t\t\tSalary";
      std::cout << "\n................................................\n";

      while (getline (MyReadFile, Info , ',')) {
        //cout << Name;     
        std::cout << Info << "\t\t" << "\t\t";
        //Output info
      }

      MyReadFile.close();
      //Close file

}
