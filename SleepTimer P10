

//https://repl.it/@Kunl19/Sleep-Timer#main.cpp

#include <iostream>       
#include <thread>         
#include <chrono>  
using namespace std;

void SleepTimer(int seconds){
  this_thread::sleep_for(std::chrono::milliseconds(seconds * seconds));
}

int main() 
{
  //std::cout << "countdown:\n";
  
  SleepTimer(10);
  //function declared 
  return 0;
}
